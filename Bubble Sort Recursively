#include <bits/stdc++.h>
using namespace std;

// T.C. = O(N^2)

void bubble_sort(std::vector<int> &v, int n)
{
	if (n == 1) return;
	for (int i = 0; i < n - 1; i++)
	{
		if (v[i] > v[i + 1]) swap(v[i], v[i + 1]);
	}
	bubble_sort(v, n - 1);
}

signed main()
{
	int t; cin >> t;
	while (t--)
	{
		int n; cin >> n; vector<int> v(n);
		for (int &i : v) cin >> i;

		bubble_sort(v, n);
		for (int i : v) cout << i << ' ';
		cout << endl;
	}
}
